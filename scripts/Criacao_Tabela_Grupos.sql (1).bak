
drop table ACTWEB.GRUPOS;
drop sequence ACTWEB.GRUPOS_ID;

CREATE TABLE ACTWEB.GRUPOS (	
  GRU_ID_GRU         NUMBER, 
  GRU_NOME           VARCHAR2(30 BYTE), 
  GRU_SIGLA          VARCHAR2(04 BYTE),
  GRU_ATIVO          VARCHAR2(1 BYTE));

ALTER TABLE ACTWEB.GRUPOS ADD ( CONSTRAINT PK_GRUPOS PRIMARY KEY (GRU_ID_GRU));

CREATE SEQUENCE ACTWEB.GRUPOS_ID
  START WITH 1
  MAXVALUE 999999999999999999999999999
  MINVALUE 1
  NOCYCLE
  CACHE 20
  NOORDER;
  